{
  "name": "giggr-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "node index",
    "dev": "export NODE_ENV=development && nodemon -r dotenv/config index",
    "test": "yarn jest --watch",
    "db:migrate": "cross-env NODE_ENV=development sequelize-cli db:create",
    "db:reset": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed --seed ./seeders/init-db",
    "sequelize:database1:migration:generate": "sequelize --options-path ./.sequelize-database1 --env database1 migration:generate",
    "sequelize:database1:migrate": "sequelize --options-path ./.sequelize-database1 --env database1 db:migrate",
    "sequelize:database1:migrate:undo": "NODE_ENV=development sequelize --options-path ./.sequelize-database1 --env database1 db:migrate:undo",
    "sequelize:database1:seed:all": "sequelize --options-path ./.sequelize-database1 --env database1 db:seed:all",
    "sequelize:database1:seed": "sequelize --options-path ./.sequelize-database1 --env database1 db:seed",
    "sequelize:master:migration:generate": "sequelize --options-path ./.sequelize-master --env master migration:generate",
    "sequelize:master:migrate": "sequelize --options-path ./.sequelize-master --env master db:migrate",
    "sequelize:master:migrate:undo": "sequelize --options-path ./.sequelize-master --env master db:migrate:undo",
    "sequelize:master:seed:all": "sequelize --options-path ./.sequelize-master --env master db:seed:all",
    "sequelize:master:seed:undo": "sequelize --options-path ./.sequelize-master --env master db:seed:undo",
    "sequelize:master:seed": "sequelize --options-path ./.sequelize-master --env master db:seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "chinese-to-pinyin": "^1.3.1",
    "contains-chinese": "^1.0.0",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "cookies": "^0.8.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "flash": "^1.1.0",
    "hyphen": "^1.6.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-sign": "^0.1.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "prompt-sync": "^4.2.0",
    "sequelize": "^6.6.2",
    "spotify-web-api-node": "^5.0.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0"
  }
}
